/entity {
  neg % added to handle X window coordinate system
  /cy exch def
  /cx exch def

  gsave
  Line_Width 3 mul setlinewidth
  cx cy translate
  newpath
  Entity_Width 2 div  neg Entity_Height 2 div neg  xmoveto
  0 Entity_Height 	xrlineto
  Entity_Width 0  	xrlineto
  0 Entity_Height neg  	xrlineto
  closepath
  stroke
  grestore
} def


/relationship {
  neg % added to handle X window coordinate system
  /cy exch def
  /cx exch def

  gsave
  Line_Width 3 mul setlinewidth
  cx cy translate
  newpath
  Rel_X_Offset neg 0 		xmoveto
  0 Rel_Y_Offset neg		xlineto
  Rel_X_Offset  0		xlineto
  0 Rel_Y_Offset		xlineto
  closepath
  stroke
  grestore
} def


/showCentered {
  dup
  stringwidth  
  pop 2 div neg 0 xrmoveto
  show
} def

/xyShowCentered { 3 1 roll xmoveto showCentered } def


/showStrImage {
  /str exch def
  /y   exch def
  /x   exch def

  gsave
  str stringwidth pop
  % calculate string width and height
  /sw exch  def
  /sw sw HpadSize 2 mul add def
  /sh FracDescent NormalFontHeight mul NormalFontHeight add def
  newpath
  x sw 2 div sub y NormalFontHeight sub	xmoveto
  sw 0  				xrlineto
  0  sh 				xrlineto
  sw neg 0				xrlineto
  closepath
  % stroke % to show bounding box
  gsave
   1 setgray
   fill
  grestore
  x y xmoveto
  str showCentered
  grestore
} def


/showStrImage2 {
  /noLines exch def
  noLines 1 eq { /line1 exch def } if
  noLines 2 eq { /line2 exch def /line1 exch def } if
  noLines 3 eq { /line3 exch def /line2 exch def /line1 exch def } if
  /my exch def
  /mx exch def

  gsave

  /h NormalFontHeight def

  noLines 1 eq {
    mx my line1 showStrImage
  } if
  noLines 2 eq {
    mx my h 2 div sub line1 showStrImage
    mx my h 2 div add line2 showStrImage
  } if
  noLines 3 eq {
    mx my h sub   line1 showStrImage
    mx my         line2 showStrImage
    mx my h add   line3 showStrImage
  } if

  grestore

} def

/xy2polar {
  /y exch def
  /x exch def
  x x mul y y mul add sqrt
  y x atan
} def

/polar2xy {
  /a exch def
  /r exch def
  r a cos mul
  r a sin mul
} def

/drawLine {
  /y2 exch def
  /x2 exch def
  /y1 exch def
  /x1 exch def

  gsave
  Line_Width setlinewidth
  newpath
  x1 y1 xmoveto
  x2 y2 xlineto
  stroke
  grestore
} def


/drawArrowHeadArc {
  /label exch def
  /y2    exch def
  /x2    exch def
  /y1    exch def
  /x1    exch def

  gsave
  Line_Width setlinewidth
  x2 x1 sub y2 y1 sub xy2polar
  /at exch def
  /ar exch def
  /ar ar Head_Size sub def
  ar at polar2xy
  /yh exch def
  /xh exch def
  /xh xh x1 add def
  /yh yh y1 add def

  % draw the base line 
  x1 y1 xmoveto
  x2 y2 xlineto
  stroke

  % draw arrow head
  xh x2 sub yh y2 sub xy2polar
  /at exch def
  /ar exch def
  /at1 at Head_Angle_Offset add def
  ar at1 polar2xy
  /ya exch def
  /xa exch def
  /xa xa x2 add def
  /ya ya y2 add def
  /at2 at Head_Angle_Offset sub def
  ar at2 polar2xy
  /yb exch def
  /xb exch def
  /xb xb x2 add def
  /yb yb y2 add def
  /vertex0x x2 def
  /vertex0y y2 def
  /vertex1x xa def
  /vertex1y ya def
  /vertex2x xb def
  /vertex2y yb def
  /vertex3x x2 def
  /vertex3y y2 def
  newpath
  vertex0x vertex0y xmoveto
  vertex1x vertex1y xlineto
  vertex2x vertex2y xlineto
  vertex3x vertex3y xlineto
  fill

  % draw the arc label
  label stringwidth pop 0 gt {
    /mid_x x2 x1 add 2 div def
    /mid_y y2 y1 add 2 div def
    mid_x mid_y label showStrImage
  } if
  grestore
} def


/drawArrowHeadArc2 {
  /noLines exch def
  noLines 1 eq { /line1 exch def } if
  noLines 2 eq { /line2 exch def /line1 exch def } if
  noLines 3 eq { /line3 exch def /line2 exch def /line1 exch def } if
  /y2      exch def
  /x2      exch def
  /my      exch def
  /mx      exch def
  /y1      exch def
  /x1      exch def

  gsave
  Line_Width setlinewidth
  x2 mx sub y2 my sub xy2polar
  /at exch def
  /ar exch def
  /ar ar Head_Size sub def
  ar at polar2xy
  /yh exch def
  /xh exch def
  /xh xh mx add def
  /yh yh my add def

  % draw the base line 
  x1 y1 xmoveto
  mx my xlineto
  mx my xmoveto
  xh yh xlineto
  stroke

  % draw arrow head
  xh x2 sub yh y2 sub xy2polar
  /at exch def
  /ar exch def
  /at1 at Head_Angle_Offset add def
  ar at1 polar2xy
  /ya exch def
  /xa exch def
  /xa xa x2 add def
  /ya ya y2 add def
  /at2 at Head_Angle_Offset sub def
  ar at2 polar2xy
  /yb exch def
  /xb exch def
  /xb xb x2 add def
  /yb yb y2 add def
  /vertex0x x2 def
  /vertex0y y2 def
  /vertex1x xa def
  /vertex1y ya def
  /vertex2x xb def
  /vertex2y yb def
  /vertex3x x2 def
  /vertex3y y2 def
  newpath
  vertex0x vertex0y xmoveto
  vertex1x vertex1y xlineto
  vertex2x vertex2y xlineto
  vertex3x vertex3y xlineto
  fill

  % draw the arc label
  line1 stringwidth pop 0 gt {
    noLines 1 eq { mx my line1 1 showStrImage2 } if
    noLines 2 eq { mx my line1 line2 2 showStrImage2 } if
    noLines 3 eq { mx my line1 line2 line3 3 showStrImage2 } if
  } if
  grestore
} def


/drawNoArrowHeadArc2 {
  /noLines exch def
  noLines 1 eq { /line1 exch def } if
  noLines 2 eq { /line2 exch def /line1 exch def } if
  noLines 3 eq { /line3 exch def /line2 exch def /line1 exch def } if
  /y2    exch def
  /x2    exch def
