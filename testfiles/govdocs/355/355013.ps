    noLines 2 eq { mx my line1 line2 2 showStrImage2 } if
    noLines 3 eq { mx my line1 line2 line3 3 showStrImage2 } if
  } if
  grestore

} def

/drawArrowHeadArcMand {
  /label exch def
  /y2    exch def
  /x2    exch def
  /y1    exch def
  /x1    exch def

  gsave
  Line_Width setlinewidth
  x2 x1 sub y2 y1 sub xy2polar
  /at exch def
