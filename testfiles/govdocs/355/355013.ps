  /vertex2y yb def
  /vertex3x x2 def
  /vertex3y y2 def
  newpath
  vertex0x vertex0y xmoveto
  vertex1x vertex1y xlineto
  vertex2x vertex2y xlineto
  vertex3x vertex3y xlineto
  fill

  % draw the arc label
  label stringwidth pop 0 gt {
    /mid_x x2 x1 add 2 div def
    /mid_y y2 y1 add 2 div def
    mid_x mid_y label showStrImage
  } if
  grestore
} def

